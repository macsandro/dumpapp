<p id="notice"><%= notice %></p>

<h1>Work Order Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_order_statuses.each do |work_order_status| %>
      <tr>
        <td><%= work_order_status.name %></td>
        <td><%= link_to 'Show', work_order_status %></td>
        <td><%= link_to 'Edit', edit_work_order_status_path(work_order_status) %></td>
        <td><%= link_to 'Destroy', work_order_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work Order Status', new_work_order_status_path %>
